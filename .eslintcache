             <Purchasing/>\n                </Route>\n                <Route path=\"/users/new\" exact>\n                    <NewUser/>\n                </Route>\n                <Route path=\"/users\" exact>\n                    <Users/>\n                </Route>\n                <Route path=\"/users/:userId\">\n                    <UpdateUser/>\n                </Route>\n\n                <Route path=\"/suppliers\" exact>\n                    <Suppliers/>\n                </Route>\n                <Route path=\"/suppliers/new\" exact>\n                    <NewSupplier/>\n                </Route>\n                <Route path=\"/suppliers/:suppId\">\n                    <UpdateSupplier/>\n                </Route>\n\n                <Route path=\"/machines\" exact>\n                    <Machines/>\n                </Route>\n                <Route path=\"/machines/new\" exact>\n                    <NewMachine/>\n                </Route>\n                <Route path=\"/machines/edit\">\n                    <UpdateMachine/>\n                </Route>\n                <Redirect to=\"/\"/>\n            </Switch>\n        );\n    } else {\n        routes = (\n            <Switch>\n\n                <Route path=\"/auth\">\n                    {/*<Auth/>*/}\n                    <AuthNew/>\n                </Route>\n                <Redirect to=\"/auth\"/>\n            </Switch>\n        );\n    }\n\n    return (\n        <AuthContext.Provider\n            value={{\n                isLoggedIn: !!token,\n                token: token,\n                userId: userId,\n                login: login,\n                logout: logout\n            }}\n        >\n            <ThemeProvider theme={theme}>\n                <Router>\n                    <MainNavigation/>\n                    <main>{routes}</main>\n                </Router>\n            </ThemeProvider>\n        </AuthContext.Provider>\n    );\n};\n\nexport default App;\n","/Users/deveenratnayake/Documents/tea_tracker_web2