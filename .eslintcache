             <Purchasing/>\r\n                </Route>\r\n                <Route path=\"/users/new\" exact>\r\n                    <NewUser/>\r\n                </Route>\r\n                <Route path=\"/users\" exact>\r\n                    <Users/>\r\n                </Route>\r\n                <Route path=\"/users/:userId\">\r\n                    <UpdateUser/>\r\n                </Route>\r\n\r\n                <Route path=\"/suppliers\" exact>\r\n                    <Suppliers/>\r\n                </Route>\r\n                <Route path=\"/suppliers/new\" exact>\r\n                    <NewSupplier/>\r\n                </Route>\r\n                <Route path=\"/suppliers/:suppId\">\r\n                    <UpdateSupplier/>\r\n                </Route>\r\n\r\n                <Route path=\"/machines\" exact>\r\n                    <Machines/>\r\n                </Route>\r\n                <Route path=\"/machines/new\" exact>\r\n                    <NewMachine/>\r\n                </Route>\r\n                <Route path=\"/machines/edit\">\r\n                    <UpdateMachine/>\r\n                </Route>\r\n                <Redirect to=\"/\"/>\r\n            </Switch>\r\n        );\r\n    } else {\r\n        routes = (\r\n            <Switch>\r\n\r\n                <Route path=\"/auth\">\r\n                    {/*<Auth/>*/}\r\n                    <AuthNew/>\r\n                </Route>\r\n                <Redirect to=\"/auth\"/>\r\n            </Switch>\r\n        );\r\n    }\r\n\r\n    return (\r\n        <AuthContext.Provider\r\n            value={{\r\n                isLoggedIn: !!token,\r\n                token: token,\r\n                userId: userId,\r\n                login: login,\r\n                logout: logout\r\n            }}\r\n        >\r\n            <ThemeProvider theme={theme}>\r\n                <Router>\r\n                    <MainNavigation/>\r\n                    <main>{routes}</main>\r\n                </Router>\r\n            </ThemeProvide